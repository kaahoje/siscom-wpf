<dx:DXWindow
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:Rest="clr-namespace:Erp.View.Rest" x:Class="Erp.View.Forms.Pessoa.PessoaFisica.PessoaFisicaFormView"
        Title="PessoaFisicaFormView" Height="740" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <Rest:RestForm Grid.Row="0" x:Name="RestCommands"/>
        <dx:DXTabControl Margin="10,11,0,-9" Grid.Row="1">
            <dx:DXTabItem Header="Tab">
                <Grid Background="Transparent"/>
            </dx:DXTabItem>
            <dx:DXTabItem Header="Tab">
                <Grid Background="Transparent"/>
            </dx:DXTabItem>
        </dx:DXTabControl>

    </Grid>
</dx:DXWindow>
